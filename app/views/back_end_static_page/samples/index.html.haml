    %noscript
      .alert.alert-block.span11
        %h4.alert-heading Warning!
        %p
          You need to have
          %a{:href => "http://en.wikipedia.org/wiki/JavaScript", :target => "_blank"} JavaScript
          enabled to use this site.
    / start: Content
    #content.span11
      .row-fluid
        .stats-date.span3
          %div Monthly Statistics
          .range 02/10/2012 - 02/11/2012
        .stats.span9
          .stat
            .left
              .number.green 1.324.996
              .title
                %span.color.green
                Visits
            .right
              .arrow
                = image_tag "back_end_static_page/uparrow.png"
              .percent +13%
          .stat
            .left
              .number.yellow 12.894.765
              .title
                %span.color.yellow
                Pageviews
            .right
              .arrow
                = image_tag "back_end_static_page/uparrow.png"
              .percent +17%
          .stat
            .left
              .number.blue 432.980
              .title
                %span.color.blue>
                Visitors
            .right
              .arrow
                = image_tag "back_end_static_page/downarrow.png"
              .percent -33%
          .stat
            .left
              .number.red 11.324
              .title
                %span.color.red>
                New Visitors
            .right
              .arrow
                = image_tag "back_end_static_page/uparrow.png"
              .percent +3%
      .row-fluid
        #stats-chart2.span12{:style => "height:300px"}
      %hr/
      .row-fluid
        .widget.span5{:ondesktop => "span5", :ontablet => "span12"}
          %h2
            %span.glyphicons.globe
              %i
            Demographics
          %hr/
          .content
            .verticalChart
              .singleBar
                .bar
                  .value
                    %span 37%
                .title US
              .singleBar
                .bar
                  .value
                    %span 16%
                .title PL
              .singleBar
                .bar
                  .value
                    %span 12%
                .title GB
              .singleBar
                .bar
                  .value
                    %span 9%
                .title DE
              .singleBar
                .bar
                  .value
                    %span 7%
                .title NL
              .singleBar
                .bar
                  .value
                    %span 6%
                .title CA
              .singleBar
                .bar
                  .value
                    %span 5%
                .title FI
              .singleBar
                .bar
                  .value
                    %span 4%
                .title RU
              .singleBar
                .bar
                  .value
                    %span 3%
                .title AU
              .singleBar
                .bar
                  .value
                    %span 1%
                .title N/A
        / /span
        .widget.span3.noMargin{:ondesktop => "span3", :ontablet => "span6"}
          %h2
            %span.glyphicons.pie_chart
              %i
            Browsers
          %hr/
          .content
            .browserStat.big
              = image_tag "back_end_static_page/browser-chrome-big.png", alt: "Chrome"
              %span 34%
            .browserStat.big
              = image_tag "back_end_static_page/browser-firefox-big.png", alt: "Firefox"
              %span 34%
            .browserStat
              = image_tag "back_end_static_page/browser-ie.png", alt: "Internet Explorer"
              %span 34%
            .browserStat
              = image_tag "back_end_static_page/browser-safari.png", alt: "Safari"
              %span 34%
            .browserStat
              = image_tag "back_end_static_page/browser-opera.png", alt: "Opera"
              %span 34%
        .widget.span4{:ondesktop => "span4", :ontablet => "span6"}
          %h2
            %span.glyphicons.charts
              %i
            Weekly Stat
          %hr/
          .content
            .sparkLineStats
              %ul.unstyled
                %li
                  %span.sparkLineStats3
                  Pageviews:
                  %span.number 781
                %li
                  %span.sparkLineStats4
                  Pages / Visit:
                  %span.number 2,19
                %li
                  %span.sparkLineStats5
                  Avg. Visit Duration:
                  %span.number 00:02:58
                %li
                  %span.sparkLineStats6
                  Bounce Rate:
                  %span.number 59,83%
                %li
                  %span.sparkLineStats7
                  \% New Visits:
                  %span.number 70,79%
                %li
                  %span.sparkLineStats8
                  \% Returning Visitor:
                  %span.number 29,21%
            / End .sparkStats
        / /span
      %hr/
      .row-fluid
        .widget.span6{:ondesktop => "span6", :ontablet => "span6"}
          %h2
            %span.glyphicons.facebook>
              %i
            Facebook Fans
          %hr/
          .content
            #facebookChart{:style => "height:300px"}
        / /span
        .widget.span6{:ondesktop => "span6", :ontablet => "span6"}
          %h2
            %span.glyphicons.twitter>
              %i
            Twitter Followers
          %hr/
          .content
            #twitterChart{:style => "height:300px"}
        / /span
      / end: Content
    / /#content.span10
  / /fluid-row
