          You need to have
          %a{:href => "http://en.wikipedia.org/wiki/JavaScript", :target => "_blank"} JavaScript
          enabled to use this site.
    / start: Content
    #content.span11
      .row-fluid
        .tempStats
          .tempStatBox.span2{:ondesktop => "span2", :ontablet => "span4"}
            .tempStat 78
            %span CPU Temperature
          .tempStatBox.span2{:ondesktop => "span2", :ontablet => "span4"}
            .tempStat 78
            %span GPU Temperature
          .tempStatBox.span2{:ondesktop => "span2", :ontablet => "span4"}
            .tempStat 78
            %span HDD Temperature
          .tempStatBox.span2.noMargin{:ondesktop => "span2", :ontablet => "span4"}
            .tempStat 78
            %span SSD Temperature
          .tempStatBox.span2{:ondesktop => "span2", :ontablet => "span4"}
            .tempStat 78
            %span Memory Bank Temperature
          .tempStatBox.span2{:ondesktop => "span2", :ontablet => "span4"}
            .tempStat 78
            %span Memory Controller Temperature
          .clearfix
        .row-fluid.hideInIE8
          .span12
            .circleStats
              .span2{:ondesktop => "span2", :ontablet => "span4"}
                .circleStatsItemBox
                  .header Disk Space Usage
                  %span.percent percent
                  .circleStat
                    %input.whiteCircle{:type => "text", :value => "58"}/
                  .footer
                    %span.count
                      %span.number 20000
                      %span.unit MB
                    %span.sep /
                    %span.value
                      %span.number 50000
                      %span.unit MB
              .span2{:ondesktop => "span2", :ontablet => "span4"}
                .circleStatsItemBox
                  .header Bandwidth
                  %span.percent percent
                  .circleStat
                    %input.whiteCircle{:type => "text", :value => "78"}/
                  .footer
                    %span.count
                      %span.number 5000
                      %span.unit GB
                    %span.sep /
                    %span.value
                      %span.number 5000
                      %span.unit GB
              .span2{:ondesktop => "span2", :ontablet => "span4"}
                .circleStatsItemBox
                  .header Memory
                  %span.percent percent
                  .circleStat
                    %input.whiteCircle{:type => "text", :value => "100"}/
                  .footer
                    %span.count
                      %span.number 64
                      %span.unit GB
                    %span.sep /
                    %span.value
                      %span.number 64
                      %span.unit GB
              .span2.noMargin{:ondesktop => "span2", :ontablet => "span4"}
                .circleStatsItemBox
                  .header CPU
                  %span.percent percent
                  .circleStat
                    %input.whiteCircle{:type => "text", :value => "83"}/
                  .footer
                    %span.count
                      %span.number 64
                      %span.unit GHz
                    %span.sep /
                    %span.value
                      %span.number 3.2
                      %span.unit GHz
              .span2{:ondesktop => "span2", :ontablet => "span4"}
                .circleStatsItemBox
                  .header Memory
                  %span.percent percent
                  .circleStat
                    %input.whiteCircle{:type => "text", :value => "100"}/
                  .footer
                    %span.count
                      %span.number 64
                      %span.unit GB
                    %span.sep /
                    %span.value
                      %span.number 64
                      %span.unit GB
              .span2{:ondesktop => "span2", :ontablet => "span4"}
                .circleStatsItemBox
