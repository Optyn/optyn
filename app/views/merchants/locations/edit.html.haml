= banner("Edit Location")

= render partial: 'merchants/merchant_managers/tabs'

#box_login
  .box_wrapper
    .box.span4.offset4
      .address 
        .head.pull-left.lead 
          = "Enter Location Details"
        .form.row-fluid
          = form_for :location, url: merchants_location_path, method: :put do |f| 
            = render :partial => 'form', :locals => {f: f}
